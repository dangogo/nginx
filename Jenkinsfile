pipeline{
    agent any
    stages {
        stage('Checkout') {
            steps{
                    echo '---This is a Checkout step---'                 
            }
        }
        stage('Build') {
            steps{
            echo '---This is a Build step---'
            }
        }
        stage('Run') {
            steps{
                    echo '---This is a run step---'    
            }
        }
    }
}
